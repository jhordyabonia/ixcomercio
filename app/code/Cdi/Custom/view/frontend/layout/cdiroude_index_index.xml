<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceContainer name="content">
        <block class="Cdi\Custom\Block\Index" name="cdiroude_index_index" template="Cdi_Custom::index.phtml" />
		<block class="Magestore\Bannerslider\Block\BannerItem" before="-">
			<action method="setBannerId">
				<argument name="bannerId" xsi:type="string">1</argument>
			</action>
		</block>
    </referenceContainer>
</page>
